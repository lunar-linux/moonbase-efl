optional_depends poppler      "--enable-poppler"   "--disable-poppler"   "for PDF support"
optional_depends libspectre   "--enable-spectre"   "--disable-spectre"   "for PS support"
optional_depends LibRaw       "--enable-raw"       "--disable-raw"       "for RAW support"
